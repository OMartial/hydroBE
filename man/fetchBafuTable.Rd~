\name{fetchBafuTable}
\alias{fetchBafuTable}

\title{
Grab daily stream flow series out of Bafu pdf tables
}

\description{
Bafu: Bundesamt fuer Umwelt, Schweiz
}

\usage{
fetchBafuTable(id = NULL, year = NULL, tempDir = NULL)
}

\arguments{
\item{id}{integer}
\item{year}{year; greater then 2003}
\item{tempDir}{directory where to write temporary files}
}

\value{
data frame with columns \code{date} and \code{qd}
}

\note{
\code{tempDir} gets not cleaned up afterwards.
}

\examples{
# to do
}